{
  "private": true,
  "scripts": {
    "dev": "max dev",
    "build": "max build",
    "postinstall": "max setup",
    "lint": "max lint",
    "lint:fix": "max lint --fix",
    "start": "npm run dev",
    "prepare": "husky install"
  },
  "dependencies": {
    "@umijs/max": "4.0.0-rc.21",
    "antd": "^4.21.0",
    "commitlint": "^17.0.2",
    "umi": "4.0.0-rc.21"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^2",
    "prettier-plugin-packagejson": "^2",
    "stylelint": "^14.8.5",
    "tailwindcss": "^3",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"husky pre-commit\" && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}